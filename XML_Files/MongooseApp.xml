<?xml version="1.0" encoding="utf-8"?>

<Configuration>
    <Name>MongooseApp</Name>
    <Url>https://registry.npmjs.org/mongoose</Url>
    <TestMode>true</TestMode>
    <Version>latest</Version>
    <TestFile>mongoose_repo.txt</TestFile>

    <ExcludeSubstring>-test</ExcludeSubstring>
    <MaxDepth>10</MaxDepth>

    <PrintLoadOrder>true</PrintLoadOrder>
    <PrintAsciiTree>true</PrintAsciiTree>

    <PlantUmlOut>out/mongoose_graph.puml</PlantUmlOut>
    <SvgOut>out/mongoose_graph.svg</SvgOut>
</Configuration>
