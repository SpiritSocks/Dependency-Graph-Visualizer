<?xml version="1.0" encoding="utf-8"?>

<Configuration>
    <Name>ExpressApp</Name>
    <Url>https://registry.npmjs.org/express</Url>
    <TestMode>true</TestMode>
    <Version>4.18.2</Version>
    <TestFile>express_repo.txt</TestFile>

    <ExcludeSubstring>-test</ExcludeSubstring>
    <MaxDepth>10</MaxDepth>

    <PrintLoadOrder>true</PrintLoadOrder>
    <PrintAsciiTree>true</PrintAsciiTree>

    <PlantUmlOut>out/express_graph.puml</PlantUmlOut>
    <SvgOut>out/express_graph.svg</SvgOut>
</Configuration>
